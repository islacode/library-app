{
  "name": "nextjs-cypress-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prettier --check . && yarn next telemetry disable && next build",
    "start": "next start",
    "lint": "next lint",
    "coverage:clean": "rm -rf coverage && rm .nyc_output/out.json",
    "component": "cross-env NEXT_TEST=1 cypress open --component",
    "component:headless": "cross-env NEXT_TEST=1 cypress run --component --env coverage=true",
    "e2e": "cross-env q=1 start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "cross-env NEXT_TEST=1 start-server-and-test dev http://localhost:3000 \"cypress run --e2e --env coverage=true\"",
    "coverage-report": "nyc report --reported=text",
    "component:coverage": "yarn component:headless && yarn coverage-report",
    "e2e:coverage": "yarn e2e:headless && yarn coverage-report",
    "test:coverage": "yarn coverage:clean && yarn component:coverage && yarn e2e:coverage",
    "storybook": "storybook dev -p 6006 --disable-telemetry",
    "build-storybook": "storybook build",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "next": "15.2.1",
    "react": "^19",
    "react-dom": "^19"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.5",
    "@cypress/code-coverage": "^3.13.12",
    "@eslint/eslintrc": "^3",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/addon-onboarding": "^8.6.4",
    "@storybook/blocks": "^8.6.4",
    "@storybook/experimental-addon-test": "^8.6.4",
    "@storybook/experimental-nextjs-vite": "8.6.4",
    "@storybook/react": "^8.6.4",
    "@storybook/test": "^8.6.4",
    "@tailwindcss/postcss": "^4",
    "@testing-library/cypress": "^10.0.3",
    "@types/node": "^22.13.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/browser": "^3.0.8",
    "@vitest/coverage-v8": "^3.0.8",
    "babel-loader": "^10.0.0",
    "babel-plugin-istanbul": "^7.0.0",
    "cross-env": "^7.0.3",
    "cypress": "^14.1.0",
    "eslint": "^8",
    "eslint-config-next": "^15.2.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-storybook": "^0.11.4",
    "husky": "^9.1.7",
    "istanbul-lib-coverage": "^3.2.2",
    "lint-staged": "^15.5.0",
    "nyc": "^17.1.0",
    "playwright": "^1.51.0",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "start-server-and-test": "^2.0.10",
    "storybook": "^8.6.4",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.0.8",
    "webpack": "^5.98.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
